{
  "rules": {
    "member-access": true,
    "no-inferrable-types": true,
    "no-internal-module": true,
    "no-var-keyword": true,
    "no-use-before-declare": true,
    "no-unused-expression": true,
    "ordered-imports": [
      true,
      {
        "import-sources-order": "lowercase-last",
        "named-imports-order": "lowercase-first"
      }
    ]
  }
}
